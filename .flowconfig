[ignore]
.*/build/.*

[include]

[libs]
flow-typed

[lints]
all=warn
nonstrict-import=error
unclear-type=error
unsafe-getters-setters=error
untyped-import=error
untyped-type-import=error

[options]
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.mjs
module.file_ext=.json
module.name_mapper.extension='sass' -> '<PROJECT_ROOT>/flow/CSSModuleStub.js'
module.name_mapper.extension='scss' -> '<PROJECT_ROOT>/flow/CSSModuleStub.js'

[strict]
